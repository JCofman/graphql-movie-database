{
  "name": "tmdb-graphql",
  "version": "0.0.1",
  "description": "A graphQL layer for the TMDB API.",
  "main": " src/server.js",
  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "precommit": "lint-staged"
  },
  "keywords": [
    "graphql",
    "apollo",
    "tbdb"
  ],
  "author": "Luke Peavey <lwpeavey@gmail.com> (http://lukepeavey.com)",
  "license": "MIT",
  "lint-staged": {
    "src/**/*.{js,css,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "apollo-datasource-rest": "^0.2.1",
    "apollo-server": "^2.2.6",
    "camelcase-keys": "^5.0.0",
    "dotenv": "^6.2.0",
    "graphql": "^14.0.2",
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "husky": "^1.2.0",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "nodemon": "^1.18.8",
    "prettier": "^1.15.3"
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "./dist"
    ],
    "coverageReporters": [
      "lcov",
      "html"
    ]
  }
}
